@Entity
@Table(name = "TBTCINFO")
public class TBTCInfo {

  @Id
  @GeneratedValue(strategy = GenerationType.IDENTITY)
  @Column(name = "TC_ID")
  private Long tcId;

  @Column(name = "TX_ID")
  private String txId;

  @Column(name = "TX_NM")
  private String txNm;

  @Column(name = "INPUT_DATA")
  private String inputData;

  @Column(name = "OUTPUT_DATA")
  private String outputData;

  // 私有构造函数
  private TBTCInfo() {
  }

  // Getter 方法

  public Long getTcId() {
    return tcId;
  }

  public String getTxId() {
    return txId;
  }

  public String getTxNm() {
    return txNm;
  }

  public String getInputData() {
    return inputData;
  }

  public String getOutputData() {
    return outputData;
  }

  // Builder 类
  public static class Builder {
    private String txId;
    private String txNm;
    private String inputData;
    private String outputData;

    public Builder setTxId(String txId) {
      this.txId = txId;
      return this;
    }

    public Builder setTxNm(String txNm) {
      this.txNm = txNm;
      return this;
    }

    public Builder setInputData(String inputData) {
      this.inputData = inputData;
      return this;
    }

    public Builder setOutputData(String outputData) {
      this.outputData = outputData;
      return this;
    }

    public TBTCInfo build() {
      TBTCInfo tBTCInfo = new TBTCInfo();
      tBTCInfo.txId = this.txId;
      tBTCInfo.txNm = this.txNm;
      tBTCInfo.inputData = this.inputData;
      tBTCInfo.outputData = this.outputData;
      return tBTCInfo;
    }
  }

  public static Builder builder() {
    return new Builder();
  }
}
